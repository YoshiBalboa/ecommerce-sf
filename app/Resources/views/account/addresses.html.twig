{% extends 'base.html.twig' %}

{% block head_title %}{{ head_title }}{% endblock %}

{% block body %}
	<div class="page-header">
		<h1>{{ h1_title }}</h1>
	</div>

	{% if addresses is empty %}
		<p>Nothing to display. <a href="{{ path('address_create') }}" title="Add an address">Add an address</a></p>
	{% else %}
		{% for address in addresses %}
			{{ dump(address) }}
		{% endfor %}
	{% endif %}
{% endblock %}
